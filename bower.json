{
    "name" : "nullpointer-i18n",
    "version" : "1.0.0-SNAPSHOT",
    "description": "i18n RequireJS-module",
    "keywords": ["i18n", "l10n", "internationalization", "localization", "js", "javascript", "requirejs"],

    "ignore": [".*", "bower_components", "lib", "node_modules", "src", "test", "tmp", "Gruntfile.js", "package.json", "README.md"],

    "dependencies": {
        "requirejs": "2.*",
        "lodash-compat": "3.2.*"
    },

    "devDependencies": {
